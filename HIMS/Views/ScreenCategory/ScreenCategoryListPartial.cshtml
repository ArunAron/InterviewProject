
@model  IEnumerable<DataCore.Models.ScreenCategory>

@{
    var count = 1;
    var CP = Convert.ToInt32(ViewBag.CurrentPagePartial);
    CP = CP - 1;
    count = (CP * DataCore.CommonClass.PageSize) + 1;

    foreach (var data in @Model)
    {
        <tr>


            <td>@count</td>
            <td>@data.ScreenCategoryName</td>
            <td> <i class="@data.Icon"></i> </td>
            <td>
                <span class="font-weight-bold">@data.UpDatedByName</span>
                <div class="text-muted font-size-sm">
                    <span class="badge badge-mark border-blue mr-1"></span>
                    @data.UpDatedDate
                </div>
            </td>
            <td class="text-center">
                <div class="list-icons">
                    <button data-popup="tooltip" title="Update" class="btn btn-sm btn-icon  rounded-round  bg-info-600 " onclick="Edit('@data.GUID')"><b><i class="icon icon-pencil7 "></i></b> </button>
                    <button data-popup="tooltip" title="Delete" class="btn  btn-sm btn-icon  rounded-round  bg-danger-800 " onclick="Delete('@data.GUID')"><b><i class="icon-trash "></i></b> </button>
                </div>
            </td>
        </tr>
        count++;
    }
}

